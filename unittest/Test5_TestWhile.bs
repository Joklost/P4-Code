#Include("stdlib.bsi")

Function Void TestWhile()
    Declare i as Integer
    i = 0
    While i < 10 Do
        PrintLine((i + 1) + "")
        i = i + 1
    End While

    i = 10
    While i > 0 Do
        PrintLine((i) + "")
        i = i - 1
    End While
End Function

Begin Declarations
End Declarations

Declare Simulations
End Declare Simulations

Begin Program

  TestWhile()

End Program
