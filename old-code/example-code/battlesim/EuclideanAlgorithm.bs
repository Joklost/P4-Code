Function Number Gcd(a as Number, b as Number)
  While NOT (b EQUALS 0) Do
    Declare t as Number
    t = b
    b = a % b
    a = t
    Return a
  End While
End Function

Begin Program
  Declare a, b as Number
  a = 1071
  b = 462

  Print(Gcd(a, b))
End Program
