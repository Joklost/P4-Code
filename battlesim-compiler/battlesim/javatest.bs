Function Integer Factorial(n as Integer)
    If n <= 1 Then
        Return 1
    End If

    Return Factorial(n - 1) * n
End Function

Function Integer Three()
    Return 3
End Function

Function Void PrintTen()
    Declare i as Integer
    i = 0
    While i < 10 Do
        PrintLine(i + "")
        i = i + 1
    End While

    PrintLine(Three() + "")

End Function

Function Void math(n as Integer)

    Declare k as Decimal
    k = 19.8

    PrintLine("TEST MATH")

    PrintLine(42 % n + "") // Comment testes
    PrintLine(42 / n + "")
    PrintLine(n++ + "")
    PrintLine(n++ + n++ + "")
    PrintLine(n-- + "")
    PrintLine(k / n + "")
    PrintLine(n / k + "")
    PrintLine(n * k + "")
    PrintLine(n++ * k-- + "")
    PrintLine(k++ % n++ + "")
    //PrintLine(k + 999999999999 + "") // Number overflow, kills it...
    PrintLine(-20 + k + "")
    PrintLine(n + -200 + "")
    PrintLine(n-- + -900 + "")
    PrintLine(n-- + -900 + "" + 100 + "")
    PrintLine(n-- + -900 + "" + 100)
    PrintLine(n-- + -900 + "" + (-3 + ""))
    PrintLine(n-- % k++ + "")

    PrintLine("Test funny equals")

